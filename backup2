--
-- TOC entry 319 (class 1255 OID 17224)
-- Name: buscar_en_esquema(text, text); Type: FUNCTION; Schema: public; Owner: ticketz
--

CREATE FUNCTION public.buscar_en_esquema(esquema_nombre text, valor_buscado text) RETURNS TABLE(tabla text, columna text, valor_encontrado text)
    LANGUAGE plpgsql
    AS $$
DECLARE
    nombre_tabla TEXT;
    nombre_columna TEXT;
    tipo_dato TEXT;
    consulta TEXT;
    encontrado BOOLEAN;
BEGIN
    FOR nombre_tabla IN
        SELECT table_name
        FROM information_schema.tables
        WHERE table_schema = esquema_nombre
          AND table_type = 'BASE TABLE'
    LOOP
        FOR nombre_columna, tipo_dato IN
            SELECT column_name, data_type
            FROM information_schema.columns
            WHERE table_schema = esquema_nombre
              AND table_name = nombre_tabla
        LOOP
            -- Construir la consulta para buscar el valor (convirtiendo a TEXT para la comparación)
            consulta := format('SELECT EXISTS (SELECT 1 FROM %I.%I WHERE CAST(%I AS TEXT) LIKE %L)',
                               esquema_nombre, nombre_tabla, nombre_columna, '%' || valor_buscado || '%');

            -- Ejecutar la consulta
            EXECUTE consulta INTO encontrado;

            -- Si se encuentra el valor, devolver la información
            IF encontrado THEN
                RETURN QUERY SELECT nombre_tabla, nombre_columna, valor_buscado;
            END IF;
        END LOOP;
    END LOOP;
    RETURN;
END;
$$;


ALTER FUNCTION public.buscar_en_esquema(esquema_nombre text, valor_buscado text) OWNER TO ticketz;

