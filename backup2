--
-- TOC entry 291 (class 1259 OID 17199)
-- Name: budibase_contacts; Type: VIEW; Schema: public; Owner: ticketz
--

CREATE VIEW public.budibase_contacts AS
 SELECT id,
    name,
    number,
    email,
    "profilePicUrl",
    "createdAt",
    "updatedAt"
   FROM public."Contacts";


ALTER VIEW public.budibase_contacts OWNER TO ticketz;

--
-- TOC entry 292 (class 1259 OID 17210)
-- Name: vw_contactos_completos; Type: VIEW; Schema: public; Owner: ticketz
--

CREATE VIEW public.vw_contactos_completos AS
 SELECT c.id,
    c.name,
    c.number AS whatsapp_number,
    tk.status AS ticket_status,
    c."createdAt",
    c."updatedAt",
    string_agg(DISTINCT (t.name)::text, ', '::text) AS tags,
    string_agg(DISTINCT (q.name)::text, ', '::text) AS queues,
    u.name AS assigned_to,
        CASE
            WHEN (EXISTS ( SELECT 1
               FROM public."Messages" m
              WHERE ((m."contactId" = c.id) AND (m.read = false)))) THEN 'Tiene mensajes no leídos'::text
            ELSE 'Sin mensajes no leídos'::text
        END AS unread_status,
    ccf.value AS additional_info,
    tn.note AS notes
   FROM (((((((public."Contacts" c
     LEFT JOIN public."TicketTags" tt ON ((c.id = tt."ticketId")))
     LEFT JOIN public."Tags" t ON ((tt."tagId" = t.id)))
     LEFT JOIN public."Tickets" tk ON ((c.id = tk."contactId")))
     LEFT JOIN public."Queues" q ON ((tk."queueId" = q.id)))
     LEFT JOIN public."Users" u ON ((tk."userId" = u.id)))
     LEFT JOIN public."ContactCustomFields" ccf ON ((c.id = ccf."contactId")))
     LEFT JOIN public."TicketNotes" tn ON ((tk.id = tn."ticketId")))
  GROUP BY c.id, c.name, c.number, tk.status, c."createdAt", c."updatedAt", u.name, ccf.value, tn.note
  ORDER BY c."updatedAt" DESC;


ALTER VIEW public.vw_contactos_completos OWNER TO ticketz;

