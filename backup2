CREATE FUNCTION public.login(email text, password text) RETURNS json
    LANGUAGE plpgsql SECURITY DEFINER
    AS $$
DECLARE
  _role name;
  result json;
BEGIN
  IF email = 'admin@ticketz.host' AND password = 'ticketz' THEN
    _role := 'authenticated';
    
    result := json_build_object(
      'role', _role,
      'token', 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJyb2xlIjoiYXV0aGVudGljYXRlZCIsImF1ZCI6IndzYXBpLnNvbW9zbGFjb211bmlkYWQuY29tIiwiZXhwIjoxNzM5Njk2MDAwfQ.4bad933cb91663ef24e2a8422342a47dc665531906bfaa35b44a0bd92260935059217098331e32b1489ae0b013360e2ae0100346c0aa67946c3b8057ad4d3bc7'
    );
    
    RETURN result;
  ELSE
    RAISE EXCEPTION 'Invalid credentials';
  END IF;
END;
$$;


